{
  "name": "lingano",
  "private": true,
  "version": "1.0.0",
  "description": "Experimental project without any npm dependencies.",
  "main": "server.js",
  "scripts": {
    "node": "node --use_strict --harmony_destructuring --harmony_default_parameters --harmony_rest_parameters --harmony_array_includes",
    "nodemon": "nodemon --use_strict --harmony_destructuring --harmony_default_parameters --harmony_rest_parameters --harmony_array_includes",
    "start": "npm run node server.js",
    "start-dev": "npm run nodemon server.js",
    "test": "npm run node scripts/tests.js",
    "test-watch": "npm run nodemon scripts/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakubMrozek/lingano.git"
  },
  "author": "Jakub Mrozek",
  "homepage": "https://github.com/JakubMrozek/lingano#readme"
}
